    "budget_full = yearly_breakdown.groupby(['title_year', 'genres_breakdown'])['budget'].sum()\n",
    "budget_full = budget_full.unstack('genres_breakdown')\n",
    "budget_full = budget_full.fillna(0.0) \n",
    "budget_full['total'] = budget_full.sum(axis = 1)\n",
